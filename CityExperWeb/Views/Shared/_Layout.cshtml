    <style>
        /*----------------------------------------------------------------------------- ANA STEP CSS -----------------------------------------------------------------------------*/
        .container .nav-tabs.wizard li {
            width: 33%;
            height: 40px;
            float: none;
            margin-bottom: 3px;
        }

            .container .nav-tabs.wizard li > * {
                position: relative;
                color: #999999;
                background-color: #dedede;
                border-color: #dedede;
            }

            .container .nav-tabs.wizard li.completed > * {
                color: #fff !important;
                background-color: #1e646c !important;
                border-color: #1e646c !important;
                border-bottom: none !important;
            }

            .container .nav-tabs.wizard li.active > * {
                color: #fff !important;
                font-family: 'Roboto',serif;
                font-size: 16px;
                background-color: #1e646c !important;
                border-color: #1e646c !important;
                border-bottom: none !important;
            }

        .container .nav-tabs.wizard a:hover {
            color: #fff;
            background-color: #2c3f4c;
            border-color: #2c3f4c
        }

        .container .nav-tabs.wizard > li > a,
        .container .nav-tabs.wizard > li > a p {
            opacity: 1;
            color: black;
            font-weight: 300 !important;
            text-align: center;
            height: 40px;
            font-size: 16px;
            font-family: 'Roboto', serif;
        }

        .container span.nmbr {
            display: inline-block;
            background: #ffffff;
            width: 20px;
            height: 20px;
            margin: auto;
            border-radius: 50%;
            font-weight: bold;
            font-size: 16px;
            color: #555;
            text-align: center;
        }


        .container .nav-tabs.wizard li {
            position: relative;
            padding: 0;
            margin: 4px 0px 4px 0;
            width: 33.3%;
            float: left;
            text-align: center;
        }

        .container .nav-tabs.wizard > li:nth-of-type(1) > a {
            border-bottom-right-radius: 8px;
            border-top-right-radius: 8px;
        }

        .container .nav-tabs.wizard li:last-child a {
            border-top-right-radius: 10px;
            border-bottom-right-radius: 10px;
        }

        .container .nav-tabs.wizard li:last-child {
            margin-right: 0;
        }

        @@media (max-width: 420px) {
            .container ul li p b {
                display: none;
            }
        }
    </style>
</head>
<body class="full-width" style="background-color:#f9f3f3;">

    @Html.Partial("_Header")
    <div class="row">
        @RenderBody()
    </div>
    @Html.Partial("_Footer")
    @*Giriş ekranım*@
</body>
</html>
