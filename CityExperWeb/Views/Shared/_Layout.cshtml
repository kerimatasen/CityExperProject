@RenderSection("Scripts", required: false)
<script>
    $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip();
    });

    $('.accordion').on('click', function () {
        $('.fa-chevron-down').removeClass('fa-chevron-down');
        $('.fa').addClass('fa-chevron-up');
    });
    $('.active').on('click', function () {
        $('.fa-chevron-up').removeClass('fa-chevron-up');
        $('.fa').addClass('fa-chevron-down');
    });

    $('.navbar-toggler').on('click', function () {
        $('.collapse:not(.show)').css('display', 'block');
    });
    $('#oneCikanListe').fadeIn();
    $('#oneCikanButton').click(function () {
        var resultMap = "";
        var resultOneCikanListe = "";
        var card = "";
        if ($('#main-map').attr('class').includes("10")) {
            resultMap = $('#main-map').attr('class').replace('10', '8');
            resultOneCikanListe = $('#oneCikanFormu').attr('class').replace('2', '4');
            $('.kerim').removeClass('col-md-12').addClass('col-md-6')
            $('#okIcon').attr("src", "https://page-assets.foxtons.co.uk/img/icons/chevron-right.svg");
            $('.hide_list').removeClass('col-md-10').addClass('col-md-8')

        }
        else {
            resultMap = $('#main-map').attr('class').replace('8', '10');
            resultOneCikanListe = $('#oneCikanFormu').attr('class').replace('4', '2');
            $('.kerim').removeClass('col-md-6').addClass('col-md-12');
            $('.hide_list').removeClass('col-md-8').addClass('col-md-10')

            $('#okIcon').attr("src", "https://page-assets.foxtons.co.uk/img/icons/chevron-left.svg");
        }
        $('#main-map').removeAttr('class');
        $('#main-map').attr('class', resultMap);

        $('#oneCikanFormu').removeAttr('class');
        $('#oneCikanFormu').attr('class', resultOneCikanListe);
    });


    //------------------------------------------------------------------------Taşınmaz Eklemede açılır kapanır alan---------------------------------------------------------------------------
    var acc = document.getElementsByClassName("accordion");
    var i;

    for (i = 0; i < acc.length; i++) {
        acc[i].addEventListener("click", function () {
            this.classList.toggle("active");
            var panel1 = this.nextElementSibling;
            if (panel1.style.display === "block") {
                panel1.style.display = "none";
            } else {
                panel1.style.display = "block";
            }
        });
    }
    $('document').ready(function () {
        var count = 0;
        $('.step').each(function () {
            count++;

        });

        var i = 0;
        //ileri
        $('#nextTapu').on('click', function () {
            if (count > i + 1) {
                i++;
                $('.active').removeClass('active');
                $('.step').eq(i).addClass('active');
                $('#mainstep1').addClass('active');

            }
        });
        $('#nextBelediye').on('click', function () {
            if (count > i + 1) {
                i++;
                $('.active').removeClass('active');
                $('.step').eq(i).addClass('active');
                $('#mainstep1').addClass('active');
            }
        });
        //geri
        $('#prevBelediye').on('click', function () {
            if (i > 0) {
                i--;
                $('.active').removeClass('active');
                $('.step').eq(i).addClass('active');
                $('#mainstep1').addClass('active');
            }
        });
        $('#nextDigerKurum').on('click', function () {
            $('#mainstep2').addClass('active');
            $('#mainstep1').removeClass('active');
        });
        //geri
        $('#prevDigerKurum').on('click', function () {
            if (i > 0) {
                i--;
                $('.active').removeClass('active');
                $('.step').eq(i).addClass('active');
                $('#mainstep2').removeClass('active');
                $('#mainstep1').addClass('active');
            }



        });

        $('#nextBina').on('click', function () {
            $('#mainstep3').addClass('active');
            $('#mainstep2').removeClass('active');
        });
        //geri
        $('#prevBina').on('click', function () {
            $('#mainstep2').removeClass('active');
            $('#mainstep1').addClass('active');
        });
        $('#nextBagimsiz').on('click', function () {
            //
        });
        //geri
        $('#prevBagimsiz').on('click', function () {
            $('#mainstep3').removeClass('active');
            $('#mainstep2').addClass('active');
        });
    });

    //---------------------------------------------------------------------------------------------------------FORM GEÇİŞ JS-------------------------------------------------------------------------------------------
    $('#nextTapu').on('click', function () {
        $('#loadingtapu').css("display", "block");
        var il = $('#ilListesi').val();
        var ilce = $('#ilceListesi').val();
        var tapuMahalle = $('#tapuMahalle').val();
        var mulkiyet = $('#mulkiyet').val();
        var hisseliDurum = $("input[name='rdHisseli']:checked").val();
        var paftaNo = $('#paftaNo').val();
        var adaNo = $('#adaNo').val();
        var parselNo = $('#parselNo').val();
        var nitelik = $('#nitelik').val();
        var tapuAlani = $('#tapuAlani').val();
        var rayicBedel = $('#rayicBedel').val();
        var zeminTipi = $('#zeminTipi').val();
        var cilt = $('#cilt').val();
        var sayfa = $('#sayfa').val();
        var tespitYapan = $('#tespitYapan').val();
        var tespitTarihi = $('#datepicker4').val();
        var aciklama = $('#parselTKAciklama').val();
        var gorusler = $('#parselTKGorus').val();

        var tapuModel = {
            il, ilce, tapuMahalle, mulkiyet, hisseliDurum, paftaNo, adaNo, parselNo, nitelik, tapuAlani, rayicBedel, zeminTipi, cilt, sayfa, tespitYapan, tespitTarihi, aciklama, gorusler,
        };
        var tapuModelJson = JSON.stringify(tapuModel);

        $.ajax({
            type: "POST",
            url: "/Gayrimenkul/ParselTapuKadastroEkle",
            contentType: "application/json; charset=utf-8",
            data: tapuModelJson,
            dataType: "json",
            success: function (data) {
                if (data.Success) {
                    $('.tapubilgileri').css('display', 'none');
                    $('.belediyeBilgileri').css('display', 'block');
                }
                else {
                    //$.notify(data.Message, "error");
                }
            },
            failure: function (errMsg) {
                alert("No response");
            }
        });
    });

    $('#nextBelediye').on('click', function () {
        $('.belediyeBilgileri').css('display', 'none');
        $('.digerKurumBilgileri').css('display', 'block');

    });
    //geri
    $('#prevBelediye').on('click', function () {
        $('.tapubilgileri').css('display', 'block');
        $('.belediyeBilgileri').css('display', 'none');

    });
    $('#nextDigerKurum').on('click', function () {
        $('#altStep').css('display', 'none');
        $('.digerKurumBilgileri').css('display', 'none');
        $('.binaBilgileri').css('display', 'block');

    });
    //geri
    $('#prevDigerKurum').on('click', function () {
        $('#altStep').css('display', 'block');
        $('.belediyeBilgileri').css('display', 'block');
        $('.digerKurumBilgileri').css('display', 'none');

    });
    $('#nextBina').on('click', function () {
        $('.bagimsizBilgileri').css('display', 'block');
        $('.binaBilgileri').css('display', 'none');


    });
    //geri
    $('#prevBina').on('click', function () {
        $('#altStep').css('display', 'block');
        $('.digerKurumBilgileri').css('display', 'block');
        $('.binaBilgileri').css('display', 'none');

    });
    $('#nextBagimsiz').on('click', function () {


    });
    //geri
    $('#prevBagimsiz').on('click', function () {
        $('.binaBilgileri').css('display', 'block');
        $('.bagimsizBilgileri').css('display', 'none');
    });
    $('#kadastroSorgu').on('click', function () {
        $('#KadastroForm').css('display', 'block');
        $('#AdresSorguForm').css('display', 'none');

    });
    $('#adresSorgu').on('click', function () {
        $('#KadastroForm').css('display', 'none');
        $('#AdresSorguForm').css('display', 'block');

    });
    $('#mobileKadastroSorgu').on('click', function () {
        $('#mobileKadastroSorguForm').css('display', 'block');
        $('#mobileAdresSorguForm').css('display', 'none');

    });
    $('#mobileAdresSorgu').on('click', function () {
        $('#mobileKadastroSorguForm').css('display', 'none');
        $('#mobileAdresSorguForm').css('display', 'block');
    });

    $('#nextKatagoriButton').on('click', function () {
        $('.ilanDetaylari').css('display', 'block');
        $('.katagoriSecimi').css('display', 'none');
        $('.satilikKiralikOnayform').css('display', 'none');
        $('#mainstep2').addClass('active');
        $('#mainstep1').removeClass('active');
    });
    $('#prevİlanDetayButton').on('click', function () {
        $('.katagoriSecimi').css('display', 'block');
        $('.ilanDetaylari').css('display', 'none');
        $('.satilikKiralikOnayform').css('display', 'none');
        $('#mainstep1').addClass('active');
        $('#mainstep2').removeClass('active');
    });
    $('#nextİlanDetayButton').on('click', function () {
        $('.katagoriSecimi').css('display', 'none');
        $('.ilanDetaylari').css('display', 'none');
        $('.satilikDuzenleForm').css('display', 'block');
        $('.satilikKiralikOnayform').css('display', 'none');
        $('#mainstep3').addClass('active');
        $('#mainstep1').removeClass('active');
        $('#mainstep2').removeClass('active');
        $('#mainstep4').removeClass('active');
    });
    $('#satilikDuzenleButton').on('click', function () {
        $('.katagoriSecimi').css('display', 'none');
        $('.ilanDetaylari').css('display', 'block');
        $('.satilikDuzenleForm').css('display', 'none');
        $('.satilikKiralikOnayform').css('display', 'none');
        $('#mainstep2').addClass('active');
        $('#mainstep1').removeClass('active');
        $('#mainstep3').removeClass('active');
        $('#mainstep4').removeClass('active');
    });

    $('#satilikDevamButton').on('click', function () {
        $('.katagoriSecimi').css('display', 'none');
        $('.ilanDetaylari').css('display', 'none');
        $('.satilikDuzenleForm').css('display', 'none');
        $('.satilikKiralikOnayform').css('display', 'block');
        $('#mainstep4').addClass('active');
        $('#mainstep1').removeClass('active');
        $('#mainstep2').removeClass('active');
        $('#mainstep3').removeClass('active');
    });
    $('#prevsatilikKiralikOnay').on('click', function () {
        $('.katagoriSecimi').css('display', 'none');
        $('.ilanDetaylari').css('display', 'none');
        $('.satilikDuzenleForm').css('display','block');
        $('.satilikKiralikOnayform').css('display','none');
        $('#mainstep3').addClass('active');
        $('#mainstep1').removeClass('active');
        $('#mainstep2').removeClass('active');
        $('#mainstep4').removeClass('active');
    });
    $('.fa-angle-down').on('click', function () {
        $('.fa-angle-down').css('display', 'none');
        $('.fa-angle-up').css('display', 'block');
    });
    $('.fa-angle-up').on('click', function () {
        $('.fa-angle-down').css('display', 'block');
        $('.fa-angle-up').css('display', 'none');
    });
    //-------------------------------------------------------- Satılık kiralık ekleme js -------------------------------------------
    $(".list-group-item").on('click', function () {
    $(this).siblings().removeClass('active')
    $(this).addClass('active');
})
</script>